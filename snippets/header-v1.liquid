
<header id="header" class="header-v2 v-sidebar">
  <div class="awemenu-nav headroom" data-responsive-width="1200">
	<div class="container container-75 hidden-xs hidden-sm">
      	<div class="vow-topbar v2 v-remove">
          <div class="topbar-left">
              <div class="has-element">
                  <div class="element-language dropdown">
                      <a id="label4" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">

                <span>{{settings.language_default}}</span>
                <span class="fa fa-caret-down f-10"></span>
              </a>
                      <ul class="dropdown-menu" aria-labelledby="label4">

                              {%for i in (1..6)%}
                              {%capture _enable%}language{{i}}_enable{%endcapture%}
                              {%capture _text%}language{{i}}_text{%endcapture%}
                              {%capture _url%}Language{{i}}_url{%endcapture%}

                              {%assign enable = settings[_enable]%}
                              {%assign text = settings[_text]%}
                              {%assign url = settings[_url]%}
                              <li><a href="{{url}}" title="{{text}}">{{text}}</a></li>
                              {% endfor %}

                      </ul>
                  </div>
                  <div class="element-currency dropdown">
                      <a id="label5" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">

                <span>{{shop.currency}}</span>
                <span class="fa fa-caret-down f-10"></span>
              </a>
                      <ul class="dropdown-menu" aria-labelledby="label5">
                          {% assign supported = settings.supported_currencies | split: ' ' %}
                          {% for c in supported %}
                          <li><a data-currency="{{c}}">{{c}}</a></li>
                          {% endfor %}
                      </ul>
                  </div>
              </div>
              <div class="has-element l-center">
                  <a href="/pages/wish-list">Wishlist</a>
              </div>
              <div class="has-element l-center">
                  <a href="#" class="hover-dropdown">My Account <b class="caret"></b></a>
                  <ul class="dropdown-menu dropdown-hover">
                      <li><a href="/account/login">Login</a></li>
                      <li><a href="/account/register">Register</a></li>
                  </ul>            
              </div>
              <div class="has-element popup-element">
                  <a href="#" class="cart open-cart">
                    {%if settings.hdv1_cart_type =='Image' %}
                      <img src="{{'hdv1_image_cart.png'|asset_url}}" alt="" class="img-reponsive">
                      {%else%}
                    <i class="{{settings.hdv1_icon_cart}} f-24"></i>
                    {% endif %}
                    <span class="count cart-count v2 enj-cartcount">{{ cart.item_count }}</span>
                  </a>
                  <a href="#" class="search-popup dropdown " data-toggle="modal" data-target="#myModal">
                    {%if settings.hdv1_search_type =='Image' %}
                      <img src="{{'hdv1_image_search.png'|asset_url}}" alt="" class="img-reponsive">
                      {%else%}
                    <i class="{{settings.hdv1_icon_search}} f-24"></i>
                    {% endif %}
                  </a>
              </div>
          </div>
          <div class="topbar-right">
              <div class="logo v3 hidden-md hidden-lg">
                <a href="{{shop.url}}" title="{{shop.name}}">
                  <img 
                  {%if settings.hdv1_logo == blank%}src="{{'hdv1_logo.png'| asset_url }}" 
                  {%else%}src="{{settings.hdv1_logo | img_url:'master'}}" class="img-reponsive"{%endif%} alt="logo" />
                </a>
              </div>
              <button type="button" class="navbar-toggle vow-icon-menu v4 js-menu icon-mobile hidden-lg hidden-md" data-toggle="collapse" data-target="#myNavbar">
                  <span class="vow-iconbar"></span>
                  <span class="vow-iconbar"></span>
                  <span class="vow-iconbar"></span>
              </button>
          </div>
		</div>
