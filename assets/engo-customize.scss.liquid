        }

        a.active {
          color: #e0545f;
          display: block;
          position: relative;
          &:before {
            content: "";
            display: block;
            width: 9px;
            height: 1px;
            background: #e0545f;
            position: absolute;
            top: 24px;
            right: 0px;
            transform: rotateZ(45deg);
          }
          &:after {
            content: "";
            display: block;
            width: 1px;
            height: 9px;
            background: #e0545f;
            position: absolute;
            top: 20px;
            right: 4px;
            transform: rotateZ(45deg);
          }
        }
      }
    }
  }
}

